        <reference name="head">
            <action method="addCss">
                <name>warehouse/boxes.css</name>
            </action>
        </reference>
    </default>
    
    <adminhtml_warehouse_index>
        <reference name="content">
            <block type="warehouse/adminhtml_warehouse" name="warehouses"/>
        </reference>
    </adminhtml_warehouse_index>
    
